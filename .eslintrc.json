{
  "extends": ["next/core-web-vitals", "eslint:recommended", "prettier"],
  "rules": {
    "no-undef": "error",
    "no-unused-vars": "warn",
    "no-console": "warn",
    "react/react-in-jsx-scope": "off",
    "react/no-unescaped-entities": 0
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  }
}
